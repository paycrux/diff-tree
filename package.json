{
  "name": "git-diff-tree",
  "version": "1.0.0",
  "description": "A CLI tool for analyzing and visualizing differences.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "git-diff-tree": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "engines": {
    "node": ">=14.16"
  },
  "keywords": [
    "git",
    "diff",
    "analyzer",
    "cli"
  ],
  "author": "Yunkuk Park",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^9.14.0",
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "inquirer": "^12.0.1",
    "ora": "^8.1.1",
    "table": "^6.8.2"
  }
}
